{
	"compilerOptions": {
		"lib": ["ESNext"],
		"module": "ESNext",
		"moduleDetection": "force",
		"moduleResolution": "bundler",
		"noEmit": true,
		"noFallthroughCasesInSwitch": true,
		"noUncheckedIndexedAccess": true,
		"skipLibCheck": true,
		"strict": true,
		"target": "ESNext",
		"verbatimModuleSyntax": true
	}
}
