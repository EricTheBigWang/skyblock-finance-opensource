		"typescript": ">= 4"
	},
	"private": true,
	"scripts": {
		"build": "tsc --build",
		"check:eslint": "bun run eslint --max-warnings=0 .",
		"check:prettier": "bun --bun run --cwd ../.. prettier --check packages/schemas",
